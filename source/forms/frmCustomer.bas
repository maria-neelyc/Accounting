Private Sub Form_Current()

Dim recClone As Recordset
Dim intNewRecord As Integer

Set recClone = Me.RecordsetClone()
intNewRecord = IsNull(Me.custID)
If intNewRecord Then
    cmdFirst.Enabled = False 'True
    cmdNext.Enabled = False
    cmdPrevious.Enabled = False 'True
    cmdLast.Enabled = False 'True
    cmdDel.Enabled = False
    'cmdAdd.Enabled = False
    Me!lblPos.Caption = "New"
    Me.AllowEdits = True
    Exit Sub
End If
'cmdAdd.Enabled = True
If recClone.RecordCount = 0 Then
    cmdFirst.Enabled = False
    cmdNext.Enabled = False
    cmdPrevious.Enabled = False
    cmdLast.Enabled = False
    cmdDel.Enabled = False
Else
    recClone.Bookmark = Me.Bookmark
    recClone.MovePrevious
    cmdFirst.Enabled = Not (recClone.BOF)
    cmdPrevious.Enabled = Not (recClone.BOF)
    recClone.MoveNext
    recClone.MoveNext
    cmdLast.Enabled = Not (recClone.EOF)
    cmdNext.Enabled = Not (recClone.EOF)
    recClone.MovePrevious
End If
recClone.Close

' Purpose: Show current record position
If Me.NewRecord Then
    Me!lblPos.Caption = "New"
Else
    Me.RecordsetClone.Bookmark = Me.Bookmark
    Me!lblPos.Caption = CStr(Me.RecordsetClone.AbsolutePosition + 1) _
                        & " of " & CStr(Me.RecordsetClone.RecordCount)
End If


    Me.AllowEdits = False
End Sub

