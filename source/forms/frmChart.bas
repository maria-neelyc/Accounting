Private Sub cmdDelete_Click()
 
    Dim strSQL As String
    Dim tempcoaRef As String
    Dim tempcoaID As Long
    Dim rs As Recordset
    
    tempcoaRef = Right(Me.xMyTreeview.SelectedItem.tag, Len(Me.xMyTreeview.SelectedItem.tag) - 2)
    tempcoaID = CLng(Right(Me.xMyTreeview.SelectedItem.Key, Len(Me.xMyTreeview.SelectedItem.Key) - 1))
    
    Select Case Left(Me.xMyTreeview.SelectedItem.tag, 1)
        Case 1
            strSQL = "SELECT tblTransactionSub.trnsID " _
                   & "FROM (tblAccount INNER JOIN tblTransactionSub ON tblAccount.accID = tblTransactionSub.accID) INNER JOIN tblChart ON tblAccount.caoID = tblChart.coaID " _
                   & "WHERE tblChart.coaRef1 = " & SplitRef(tempcoaRef, 1)
        Case 2
            strSQL = "SELECT tblTransactionSub.trnsID " _
                   & "FROM (tblAccount INNER JOIN tblTransactionSub ON tblAccount.accID = tblTransactionSub.accID) INNER JOIN tblChart ON tblAccount.caoID = tblChart.coaID " _
                   & "WHERE tblChart.coaRef1 = " & SplitRef(tempcoaRef, 1) & " AND " _
                   & "      tblChart.coaRef2 = " & SplitRef(tempcoaRef, 2)
        Case 3
            strSQL = "SELECT tblTransactionSub.trnsID " _
                   & "FROM (tblAccount INNER JOIN tblTransactionSub ON tblAccount.accID = tblTransactionSub.accID) INNER JOIN tblChart ON tblAccount.caoID = tblChart.coaID " _
                   & "WHERE tblChart.coaRef1 = " & SplitRef(tempcoaRef, 1) & " AND " _
                   & "      tblChart.coaRef2 = " & SplitRef(tempcoaRef, 2) & " AND " _
                   & "      tblChart.coaRef3 = " & SplitRef(tempcoaRef, 3)
        Case 4
            strSQL = "SELECT tblTransactionSub.trnsID " _
                   & "FROM (tblAccount INNER JOIN tblTransactionSub ON tblAccount.accID = tblTransactionSub.accID) INNER JOIN tblChart ON tblAccount.caoID = tblChart.coaID " _
                   & "WHERE tblChart.coaRef1 = " & SplitRef(tempcoaRef, 1) & " AND " _
                   & "      tblChart.coaRef2 = " & SplitRef(tempcoaRef, 2) & " AND " _
                   & "      tblChart.coaRef3 = " & SplitRef(tempcoaRef, 3) & " AND " _
                   & "      tblChart.coaRef4 = " & SplitRef(tempcoaRef, 4)
        Case 5
            strSQL = "SELECT tblTransactionSub.trnsID " _
                   & "FROM (tblAccount INNER JOIN tblTransactionSub ON tblAccount.accID = tblTransactionSub.accID) INNER JOIN tblChart ON tblAccount.caoID = tblChart.coaID " _
                   & "WHERE tblChart.coaID = " & tempcoaID
    End Select

    Set rs = CurrentDb.OpenRecordset(strSQL)
    
    If rs.RecordCount > 0 Then
       MsgBox FnIsErr(7001), vbExclamation
       rs.Close
       Exit Sub
    End If
    
    strSQL = "SELECT coaID " _
           & "FROM tblChart " _
           & "WHERE coaIDg = " & tempcoaID _
           & " AND coaID <> " & tempcoaID
           
    Set rs = CurrentDb.OpenRecordset(strSQL)
    
    If rs.RecordCount > 0 Then
       MsgBox FnIsErr(7002), vbExclamation
       rs.Close
       Exit Sub
    End If
           
    strSQL = "SELECT accID " _
           & "FROM tblAccount " _
           & "WHERE caoID = " & tempcoaID
           
    Set rs = CurrentDb.OpenRecordset(strSQL)
    
    If rs.RecordCount > 0 Then
        If MsgBox(FnIsErr(7003), vbOKCancel, "Delete Reference") = vbOK Then
            DoCmd.SetWarnings False
            strSQL = "DELETE from tblChart WHERE coaID=" & tempcoaID
            FnLog (strSQL)
            DoCmd.RunSQL (strSQL)
            DoCmd.SetWarnings True
            If Me.xMyTreeview.Nodes.Count > 0 Then
                If Me.xMyTreeview.SelectedItem.Previous Is Nothing Then
                    PopulateTreeView (Me.xMyTreeview.SelectedItem.Parent.Key)
                Else
                    PopulateTreeView (Me.xMyTreeview.SelectedItem.Previous.Key)
                End If
            Else
                Me.xMyTreeview.Nodes.Clear
            End If
        End If
    Else
        If MsgBox(FnIsErr(7004), vbOKCancel, "Delete Reference") = vbOK Then
            DoCmd.SetWarnings False
            strSQL = "DELETE from tblChart WHERE coaID=" & tempcoaID
            FnLog (strSQL)
            DoCmd.RunSQL (strSQL)
            DoCmd.SetWarnings True
            If Me.xMyTreeview.Nodes.Count > 1 Then
                If Me.xMyTreeview.SelectedItem.Previous Is Nothing Then
                    PopulateTreeView (Me.xMyTreeview.SelectedItem.Parent.Key)
                Else
                    PopulateTreeView (Me.xMyTreeview.SelectedItem.Previous.Key)
                End If
            Else
                Me.xMyTreeview.Nodes.Clear
            End If
        End If
    End If
    rs.Close
End Sub

