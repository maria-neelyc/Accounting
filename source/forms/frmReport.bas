Function ChangeCbo(RowSource, ByRef Bound) As String
    intOrdPos = InStr(RowSource, "ORDER")
    intColStart = 7
    intColEnd = InStr(RowSource, "FROM") - 1
    strCols = Right(Left(RowSource, intColEnd), Len(Left(RowSource, intColEnd)) - 7)
    arrCols = Split(strCols, ",")
    strOldOrder = Trim(Right(RowSource, Len(RowSource) - intOrdPos - 7))
    Dim intColNum As Integer
    strNewSelect = ""
    strBound = Trim(arrCols(Bound - 1))
    intColNum = UBound(arrCols) + 1
    intBound = 1
    For i = (intColNum - 1) To 0 Step -1
        If (Trim(arrCols(i)) <> strOldOrder) Then
            RowSource = Left(RowSource, intOrdPos + 7) & " " & Trim(arrCols(i))
        End If
        If (Trim(arrCols(i)) = strBound) Then
            Bound = intBound
        End If
        strNewSelect = strNewSelect & Trim(arrCols(i)) & ", "
        intBound = intBound + 1
    Next i
    strNewSelect = Left(strNewSelect, Len(strNewSelect) - 2)
    ChangeCbo = Left(RowSource, intColStart) _
                          & " " & strNewSelect _
                          & " " & Right(RowSource, Len(RowSource) - InStr(RowSource, "FROM") + 1)
    
    'Requery
End Function
